{
  "name": "data-structs-n-algos",
  "version": "3.0.0",
  "description": "JavaScript and TypeScript implementations for common data structures and algorithms.",
  "main": "dist/index.js",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll",
